{
  "name": "rubiks-timer",
  "version": "1.0.0",
  "description": "Timing your solves.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rubiksron/rubiks-timer.git"
  },
  "keywords": [
    "reactjs"
  ],
