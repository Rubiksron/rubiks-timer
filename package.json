  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rubiksron/rubiks-timer/issues"
  },
  "homepage": "https://github.com/Rubiksron/rubiks-timer#readme",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
